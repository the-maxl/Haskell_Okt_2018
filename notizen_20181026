List comprehension
[2^x | x <- [1..10]]
[2,4,8,16,32,64,128,256,512,1024]

 [x | x <- [1..5], x > 2]
[3,4,5]

 [(x,y) | x <- [1..3], y <- [1..3]]
[(1,1),(1,2),(1,3),(2,1),(2,2),(2,3),(3,1),(3,2),(3,3)]

[(x,y) | x <- [1..3], y <- [1..3], y >= x]
[(1,1),(1,2),(1,3),(2,2),(2,3),(3,3)]



 [(x,y) | x <- [4], y <- [1..], y==x+x]
[(4,8)


error = 1e-2
 take 15 (zipWith (*) [1..] (repeat error))
[1.0e-2,2.0e-2,3.0e-2,4.0e-2,5.0e-2,6.0e-2,7.0e-2,8.0e-2,9.0e-2,0.1,0.11,0.12,0.13,0.14,0.15]
:

